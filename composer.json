{
    "name": "arifszn/laravel-advanced-validation",
    "description": "Laravel advanced validation rules for real-life scenarios.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Arifszn\\AdvancedValidation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arifszn\\AdvancedValidation\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Ariful Alam",
            "email": "swazan.arif@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arifszn\\AdvancedValidation\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "keywords": [
        "laravel",
        "validation",
        "validator",
        "laravel-framework",
        "laravel-package",
        "laravel-validation",
        "validation-library",
        "validation-rules",
        "laravel-validation-rules",
        "validation-tool",
        "laravel-validator",
        "laravel-rule",
        "laravel-ruleset",
        "laravel-validation-message",
        "laravel-validation-library",
        "laravel-advanced-validation"
    ]
}
